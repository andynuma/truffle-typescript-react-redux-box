{
  "name": "typescript-react-redux-truffle-box",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.3.1",
    "react-toastify": "^4.4.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.1",
    "solc": "^0.5.8",
    "styled-components": "^5.0.1",
    "truffle": "^5.1.13",
    "truffle-hdwallet-provider": "^1.0.2",
    "typescript": "~3.7.2",
    "web3": "^1.2.6",
    "web3-eth-contract": "^1.2.6",
    "web3-utils": "^1.2.6"
  },
  "scripts": {
    "dev": "npm run artifacts && yarn start",
    "mc": "truffle compile --all && truffle migrate --reset",
    "mcr": "truffle compile --all && truffle migrate --network ropsten",
    "artifacts": "cp -r ./build/contracts/ ./src/contractJson/",
    "del": "rm -rf ../build/contracts && rm -rf ./src/contracts",
    "set": "npm run del && npm run mc && npm run dev",
    "t": " npm run del && npm run mc && truffle test",
    "start": "react-scripts start && npm run artifacts",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mcdev": "npm run mc && npm run dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "typechain": "^1.0.5",
    "typechain-target-web3-v1": "^1.0.4",
    "web3-typescript-typings": "^0.10.2"
  }
}